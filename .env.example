# MeoBeo Agent environment example
# Copy this file to .env and fill in your secrets

# Google Gemini API key (required)
GOOGLE_API_KEY=

# Qdrant configuration
# For local Qdrant via Docker: http://localhost:6333
# For Qdrant Cloud: use your HTTPS URL and set QDRANT_API_KEY
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION=knowledge_base

# RAG configuration
RAG_TOP_K=5
RAG_CONTEXT_CHAR_LIMIT=8000

# Server configuration
# MeoBeo runs on port 9995 by default
PORT=9995
